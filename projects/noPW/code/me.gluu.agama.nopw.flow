Flow me.gluu.agama.nopw
     Basepath "nopw"
creds = {}
uidResult = RRF "uid-form.ftlh" creds.username
isBob = Call info.gluu.qa.authn.PersonChecker#isBob creds.username
When isBob is true
     steptwoResult = RRF "stepTwo.ftlh" next
     Log "@error Alert: Bob tried to login"
     Finish false
Log "@info Not bob... it's: " creds.username 
Finish true